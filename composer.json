{
  "name": "greeflas/sorter",
  "description": "This is simple library for sorting array of data.",
  "type": "project",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Volodymyr Kupriienko",
      "email": "vladimir.kuprienko@itea.ua"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13"
  },
  "autoload": {
    "psr-4": {
      "Greeflas\\Sorter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Greeflas\\Sorter\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix --allow-risky=yes --diff"
  }
}
